{
    "name": "api-meusacolao",
    "version": 2,
    "public": true,
    "builds": [
        {
          "src": "src/*.js",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/api/(.*)",
          "dest": "src/api/$1"
        },
        {
          "src": "/(.*)",
          "dest": "src/index.js"
        }
      ]
    }